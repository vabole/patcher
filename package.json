{
  "name": "patcher",
  "version": "0.1.0",
  "description": "Tool for patching npm packages",
  "main": "src/index.js",
  "bin": {
    "patcher": "./src/cli.js"
  },
  "scripts": {
    "patcher": "node src/cli.js",
    "test": "node local-test-runner.js",
    "test:patch": "node src/cli.js local-config.json",
    "test:undo": "node src/cli.js --undo local-config.json"
  },
  "keywords": [
    "patch",
    "npm",
    "monkey-patch"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "is-odd": "^3.0.1",
    "js-beautify": "^1.14.9"
  }
}
